    <div class="modal fade" id="nuevoUser" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content  modal-dialog-centered position-absolute top-50 start-10">
                <div class="modal-header">
                    <h1 class="modal-title fs-5" id="exampleModalLabel">Nuevo usuario</h1>
                   
                </div>
                <div class="modal-body">

                    <form class="row g-3 was-validated">
                        <div class="col-md-6">
                            <label for="validationServer01" class="form-label">Nombre</label>
                            <input type="text" class="form-control is-valid" id="validationServer01" value="Mark"
                                pattern="[A-Za-zÀ-ÿ]+">
                            <div class="valid-feedback">

                            </div>
                            <div class="invalid-feedback">
                                Ingresa un valor correcto
                            </div>
                        </div>

                        <div class="col-md-6">
                            <label for="validationServerUsername" class="form-label">Correo</label>
                            <div class="input-group  is-invalid  ">
                                <span class="input-group-text" id="inputGroupPrepend3">@</span>
                                <input type="email" class="form-control " id="validationServerUsername"
                                    pattern="[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?(?:\.[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9]"
                                    aria-describedby="inputGroupPrepend3 validationServerUsernameFeedback" required>
                                <div class="invalid-feedback">
                                    Ingrese un valor correcto
                                </div>
                            </div>

                        </div>



                        <div class="col-md-6">
                            <label for="validationServer03" class="form-label">Teléfono</label>
                            <input type="number" class="form-control is-invalid" id="validationServer03" require
                                pattern="[0-9]{6,10}">
                            <div class="invalid-feedback">
                                Ingrese un telefono adecuado
                            </div>
                        </div>
                        <div class="col-md-3">
                            <label for="validationServer04" class="form-label">Estado</label>
                            <select class="form-select is-invalid" id="validationServer04"
                                aria-describedby="validationServer04Feedback">
                                <option selected disabled value="">Activo</option>
                                <option>Inactivo</option>
                            </select>
                        </div>
                        <div class="col-md-3">
                            <label for="validationServer05" class="form-label">Rol</label>
                            <select class="form-select is-invalid" id="validationServer04"
                                aria-describedby="validationServer04Feedback">
                                <option>Cliente</option>
                                <option>Empleado</option>
                                <option>Admin</option>
                                <option>S. usuario</option>
                            </select>
                        </div>
                        <div class="col-md-6">
                            <label for="validationServer03" class="form-label">Contraseña</label>
                            <input type="number" class="form-control is-invalid" id="validationServer03" require
                                pattern="[0-9]{6,10}">
                            <div class="invalid-feedback">
                                Ingrese una contraseña adecuada
                            </div>
                        </div>
                        <div class="col-md-6">
                            <label for="validationServer03" class="form-label">Confirmar contraseña</label>
                            <input type="number" class="form-control is-invalid" id="validationServer03" require
                                pattern="[0-9]{6,10}">
                            <div class="invalid-feedback">
